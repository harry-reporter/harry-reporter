{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-microsoft-contrib"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "always"],
    "quotemark": [true, "single"],
    "no-unsafe-any": false,
    "no-implicit-dependencies": false,
    "missing-jsdoc": false
  },
  "rulesDirectory": ["node_modules/tslint-microsoft-contrib"]
}
