{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "always"],
    "quotemark": [true, "single"],
    "no-unsafe-any": false,
    "no-implicit-dependencies": false,
    "missing-jsdoc": false,
    "ordered-imports": false,
    "no-console": false
  }
}
