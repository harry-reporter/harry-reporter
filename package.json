  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@types/node": "^10.12.10",
    "app-module-path": "^2.2.0",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
