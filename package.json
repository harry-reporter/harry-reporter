  "description": "Reporter plugin for hermione",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "dev": "webpack-dev-server --mode development",
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc -w",
    "lint": "npm run lint:styles && npm run lint:ts",
    "lint:styles": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harry-reporter/harry-reporter.git"
  },
  "keywords": [
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/harry-reporter/harry-reporter/issues"
  },
  "homepage": "https://github.com/harry-reporter/harry-reporter#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@types/bluebird": "^3.5.24",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.10",
    "@types/opener": "^1.4.0",
    "app-module-path": "^2.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
