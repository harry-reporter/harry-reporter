{
  "name": "harry-reporter",
  "version": "2.1.1",
  "description": "Reporter plugin for hermione",
  "main": "lib/index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:core dev:static",
    "dev:core": "tsc -w",
    "dev:static": "cd src/static && cross-env GUI=true webpack-dev-server -d --config=webpack.dev.js",
    "build": "rm -rf lib && npm run build:core && npm run build:static",
    "build:core": "rm -rf lib && tsc",
    "build:static": "cd src/static && webpack -p --config=webpack.prod.js",
    "lint": "npm run lint:ts",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "lint:static": "tslint './src/static/**/*.ts*' --format stylish -c './src/static/tslint.json'",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harry-reporter/harry-reporter.git"
  },
  "keywords": [
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/harry-reporter/harry-reporter/issues"
  },
  "homepage": "https://github.com/harry-reporter/harry-reporter#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@types/bluebird": "^3.5.24",
    "@types/classnames": "^2.2.6",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.10",
    "@types/opener": "^1.4.0",
    "@types/react": "^16.7.9",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-virtualized": "^9.18.9",
    "@types/urijs": "^1.15.38",
    "app-module-path": "^2.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
