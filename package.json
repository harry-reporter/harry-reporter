{
  "name": "harry-reporter",
  "version": "1.0.0",
  "description": "Reporter plugin for hermione",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "rm -rf lib && tsc",
    "lint": "npm run lint:styles && npm run lint:ts",
    "lint:styles": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harry-reporter/harry-reporter.git"
  },
  "keywords": [
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/harry-reporter/harry-reporter/issues"
  },
  "homepage": "https://github.com/harry-reporter/harry-reporter#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/node": "^10.12.10",
    "app-module-path": "^2.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
