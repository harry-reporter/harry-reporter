    "opener": "^1.5.1",
    "primer": "^10.10.2",
    "primer-buttons": "^2.6.2",
    "react": "^16.6.3",
    "react-clipboard.js": "^1.1.4",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "remote-origin-url": "^2.0.0",
    "reselect": "^4.0.0",
    "signal-exit": "^3.0.2",
